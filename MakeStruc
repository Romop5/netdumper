CC=gcc
CFLAGS=-g -Wall -g -std=c99
all: test deamon nfaltern

nfaltern: query.o hashtbl.o queue.o udp.o
	gcc query.o hashtbl.o queue.o udp.o nfaltern.c -l:libnf.a -o nfaltern
#query: query.o udp.o hashtbl.o queue.o
#	gcc query.o hashtbl.o queue.o udp.o -o query
deamon: deamon.o hashtbl.o queue.o udp.o
	gcc netstat.c deamon.o hashtbl.o queue.o udp.o -o deamon
test: hashtbl.o queue.o
	gcc test.c hashtbl.o queue.o -o test 

clean: 
	rm deamon query test
	
hashtbl.o: hashtbl.c
queue.o: queue.c queue.h
server: server.c
	gcc -o server server.c

deamon.o: deamon.c queue.h structure.h hashtbl.h udp.h

udp.o: udp.c udp.h
query.o: query.c udp.h structure.h
